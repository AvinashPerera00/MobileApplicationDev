<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/mad"
    tools:context=".updateSuccessfullpage">

    <TextView
        android:id="@+id/SuccessfullMessage"
        android:layout_width="294dp"
        android:layout_height="138dp"
        android:layout_marginTop="312dp"
        android:fontFamily="sans-serif-medium"
        android:text="@string/SuccessfullMessage"
        android:textSize="42sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:background="@android:color/holo_blue_light"
        android:id="@+id/gobackButton"
        android:layout_width="183dp"
        android:layout_height="54dp"
        android:layout_marginTop="64dp"
        android:text="@string/gobackButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.451"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/SuccessfullMessage" />
</androidx.constraintlayout.widget.ConstraintLayout>